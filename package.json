{
  "name": "@-ui/transition",
  "version": "0.2.1",
  "homepage": "https://github.com/dash-ui/transition#readme",
  "repository": "github:dash-ui/transition",
  "bugs": "https://github.com/dash-ui/transition/issues",
  "author": "Jared Lunde <jared.lunde@gmail.com> (https://jaredLunde.com)",
  "license": "MIT",
  "description": "A library for creating CSS transitions with -ui",
  "keywords": [
    "css transition",
    "css-in-js transition",
    "css-in-js animation",
    "css animation",
    "css spring"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/cjs/index.d.ts",
  "umd:main": "dist/umd/-ui-transition.min.js",
  "unpkg": "dist/umd/-ui-transition.min.js",
  "files": [
    "/dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:cjs && npm run build:es && npm run build:umd && npm run build:types",
    "build:cjs": "babel src -d dist/cjs -x .ts --ignore \"**/*.test.ts\",\"**/test.ts\" --delete-dir-on-start",
    "build:es": "babel src -d dist/es -x .ts --env-name es --ignore \"**/*.test.ts\",\"**/test.ts\" --delete-dir-on-start",
    "build:types": "tsc -p tsconfig.json -d --outDir dist/es --emitDeclarationOnly && mkdir -p dist/cjs && cp -R dist/es/**.d.ts dist/cjs && rimraf dist/**/*.test.d.ts",
    "build:umd": "rollup -c",
    "check-types": "tsc --noEmit -p tsconfig.json",
    "format": "prettier --write \"**/*.{ts,js,md,yml,json,babelrc,eslintrc,prettierrc}\"",
    "lint": "eslint . --ext .ts",
    "prepublishOnly": "npm run lint && npm run test && npm run build && npm run format",
    "test": "jest",
    "validate": "npm run check-types && npm run lint && npm run test -- --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run build:types"
    }
  },
  "lint-staged": {
    "**/*.{ts,js}": [
      "eslint",
      "prettier --write"
    ],
    "**/*.{md,yml,json}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@-ui/styles": "^1.0.51",
    "@lunde/babel-preset-es": "latest",
    "@rollup/plugin-replace": "^2.3.0",
    "@types/jest": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "cross-env": "latest",
    "eslint": "latest",
    "eslint-import-resolver-jest": "latest",
    "eslint-plugin-jest": "latest",
    "husky": "latest",
    "jest": "latest",
    "lint-staged": "latest",
    "prettier": "latest",
    "rimraf": "^2.6.3",
    "rollup": "^1.28.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.3",
    "ts-jest": "latest",
    "typescript": "latest"
  },
  "dependencies": {},
  "peerDependencies": {
    "@-ui/styles": "^1.0.51"
  }
}
